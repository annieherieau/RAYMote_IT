<% content_for :title, "Bienvenue" %>
<main class="rbt-main-wrapper">
    <!-- Start Banner Area -->
    <section class="rbt-banner-area rbt-banner-1">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12 pb--120 pt--10">
                    <div class="content">
                        <div class="inner rbt-section-gap">
                            
                            <h1 class="title">
                                <span class="theme-gradient">R</span><span>evolutionize</span> <span class="theme-gradient">A</span><span>ll</span> <span class="theme-gradient">Y</span><span>our</span> <span class="theme-gradient">M</span><span>astery</span>
                            </h1>
                            <p class="text"> 
                                Plongez dans une expérience d'e-learning innovante qui bouleverse les normes établies et propulse<br> votre apprentissage vers de nouveaux sommets. Transformez vos ambitions en réalité en rejoignant <strong><br>la révolution de l'éducation</strong> aujourd'hui.
                            </p> 

                            <div class="d-flex justify-content-start align-items-center mt-3">
                            
                                <div class="slider-btn">
                                    <a class="rbt-moderbt-btn" href="<%=new_user_registration_path%>">
                                        <span class="moderbt-btn-text" style="">
                                            Start the revolution now
                                        </span>
                                        <span class="btn-icon"><i class="fa-solid fa-arrow-right"></i></span>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End Banner Area -->

   

    <!-- Categories Courses -->
    <section class="rbt-categories-area bg-color-white rbt-section-gapBottom">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title text-center">
                        <span class="subtitle bg-primary-opacity">LES CATÉGORIES POPULAIRES</span>
                        <h2 class="title">Explorez Les Top Catégories<br/> De Nos Créateurs Superactifs</h2>
                    </div>
                </div>
            </div>

            
            <div class="row g-5 mt--20">
                <!-- Start Category Box Layout  -->
                <% @top_categories.each do |category| %>
                    <div class="col-lg-3 col-md-6 col-sm-6 col-12">
                        <a class="rbt-cat-box rbt-cat-box-1 text-center" href= "<%= category_path(category) %>">
                            <div class="inner">
                                <div class="icons">
                                    <% if category.icon.attached? %>
                                    <%= image_tag category.icon, alt: "Icons Images" %>
                                    <% end %>
                                </div>
                                <div class="content">
                                    <h4 class="title">
                                    <%= category.name %>      
                                    </h4>
                                    <div class="read-more-btn">
                                        <span class="rbt-btn-link"> <%= category.workshops.count %> cours</span>
                                        <span class="btn-icon"><i class="fa-solid fa-arrow-right" style="font-size: 0.6em;"></i></span>
                                    </div>
                                </div>
                            </div>
                        </a>
                    </div>            
                <% end %>
            </div>
        </div> 
    </section>

    <!-- End Category Box Layout  -->
    <!-- End Categories Courses -->



    <!-- Start Workshops COURS Area -->
    <section class="rbt-course-area bg-color-extra2 rbt-section-gap">
        <div class="container">
            <div class="row mb--60">
                <div class="col-lg-12">
                    <div class="section-title text-center">
                        <span class="subtitle bg-secondary-opacity">les cours populaires</span>
                        <h2 class="title">Les Tops Cours Sélectionnés<br/> Par La RAYM Community</h2>
                    </div>
                </div>
            </div>

            <!-- Start Single Course  -->
            <div class="row g-5">
                <div class="container">
                    <div id="workshops">
                        <div class="row mb-4 justify-content-around">
                            <% @courses.each do |workshop| %>
    
                                <div class="col-lg-4 col-md-6 col-sm-6 col-12">
                                    <%= render partial: "workshops/workshop_card", locals: {workshop: workshop} %>
                                </div>
                                            
                            <% end %>
                        </div>
                        <div class="contact-btn mt--80 d-flex justify-content-center">
                            <a class="rbt-btn hover-icon-reverse btn-gradient radius-round" href="<%= workshops_path(event: false) %>">
                                <div class="icon-reverse-wrapper">
                                    <span class="btn-text">Voir tous les cours</span>
                                    <span class="btn-icon"><i class="fa-solid fa-arrow-right"></i></span>
                                    <span class="btn-icon"><i class="fa-solid fa-arrow-right"></i></span>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div> 
    </section> 

    <!-- End Workshops Area -->



    <!-- Start Workoshop ATELIERS Live Area -->
    <section class="rbt-course-area bg-color-extra2 rbt-section-gap">
        <div class="container">
            <div class="row mb--60">
                <div class="col-lg-12">
                    <div class="section-title text-center">
                        <span class="subtitle bg-coral-opacity">Les Prochains Ateliers </span>
                        <h2 class="title">En Intéraction Avec Nos Formateurs<br/>Live Streaming</h2>
                    </div>
                </div>
            </div>
        
            <!-- Start Call To Action  -->
            <div class="rbt-callto-action-area mt-5">
                <div class="container">
                    <div class="row g-5">
                        <!-- EVENT card-->
                        <% @events.each do |event| %>
                            
